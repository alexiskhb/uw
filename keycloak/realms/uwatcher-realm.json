            "webOrigins": [
                "http://localhost"
            ]
        },
        {
            "clientId": "oauth2-proxy",
            "enabled": true,
            "publicClient": false,
            "secret": "oP50OnQsgA82NOemthEHNKxgs4oL1OkQ",
            "redirectUris": [
                "http://localhost/*"
            ],
            "webOrigins": [
                "http://localhost"
            ]
        }
    ],
